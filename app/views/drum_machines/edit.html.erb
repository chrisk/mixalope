<h1>Editing drum_machine</h1>

<% form_for(@drum_machine) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @drum_machine %> |
<%= link_to 'Back', drum_machines_path %>
